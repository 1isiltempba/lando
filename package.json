  "scripts": {
    "build:docs": "yarn generate:api && node ./scripts/docs.js && vuepress build docs -d _docs",
    "generate:api": "node ./scripts/yaml2json.js ./sponsors.yml ./contributors.yml --output-dir docs/.vuepress/public/api",
    "lint:metrics": "eslint --quiet --no-ignore --config .eslintrc.metrics.json metrics",
    "lint:docs": "eslint  --quiet --ext .js,.vue --config .eslintrc.vuepress.json docs/.vuepress",
    "release": "bump --prompt --tag --all --push",
    "dev:docs": "yarn generate:api && node ./scripts/docs.js && vuepress dev --temp /tmp/lando-docs --cache /tmp/lando-docs-cache --port 8021 docs",
    "dev:metrics": "./node_modules/nodemon/bin/nodemon.js ./metrics/server.js",
  },
